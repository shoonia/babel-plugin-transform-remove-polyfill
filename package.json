    "babel",
    "transform",
    "optimization",
    "es6",
    "es2015",
    "modern",
    "dead-code-elimination",
    "feature-detection",
    "bundle-size"
  ],
  "author": {
    "name": "Alexander Zaytsev",
    "email": "zaytsev126@gmail.com",
    "url": "https://x.com/_shoonia"
